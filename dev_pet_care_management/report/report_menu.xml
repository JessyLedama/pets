<odoo>

    <record id="paperformat_dev_pet_service_mana" model="report.paperformat">
            <field name="name">A4 Pet</field>
            <field name="default" eval="True" />
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">38</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">30</field>
            <field name="dpi">90</field>
        </record>
        
        
    <record id="menu_print_pet_service" model="ir.actions.report">
        <field name="name">Print Service</field>
        <field name="model">dev.pet.service</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">dev_pet_care_management.dev_pet_service_report</field>
        <field name="report_file">dev_pet_care_management.dev_pet_service_report</field>
        <field name="binding_model_id" ref="dev_pet_care_management.model_dev_pet_service"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="paperformat_dev_pet_service_mana"/>
    </record>
    
    <record id="menu_print_pet_boarding" model="ir.actions.report">
        <field name="name">Print Boarding</field>
        <field name="model">dev.pet.boarding</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">dev_pet_care_management.dev_pet_boarding_report</field>
        <field name="report_file">dev_pet_care_management.dev_pet_boarding_report</field>
        <field name="binding_model_id" ref="dev_pet_care_management.model_dev_pet_boarding"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="paperformat_dev_pet_service_mana"/>
    </record>
    
</odoo>
