<odoo>
    <data noupdate="1">
        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">rest.seq.patient</field>
            <field name="prefix">APN%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_case" model="ir.sequence">
            <field name="name">Case Sequence</field>
            <field name="code">patient.case</field>
            <field name="prefix">CS%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="grooming_details_sequence" model="ir.sequence">
            <field name="name">Grooming Sequence</field>
            <field name="code">grooming.details</field>
            <field name="prefix">G%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="training_details_sequence" model="ir.sequence">
            <field name="name">Training Sequence</field>
            <field name="code">training.details</field>
            <field name="prefix">T%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">hospital.prescription</field>
            <field name="active">TRUE</field>
            <field name="prefix">MDPR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hospital_surgery" model="ir.sequence">
            <field name="name">Surgery Sequence</field>
            <field name="code">hospital.surgery</field>
            <field name="active">TRUE</field>
            <field name="prefix">HPS/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="maintenance_sequence" model="ir.sequence">
            <field name="name">Maintenance Sequence Number</field>
            <field name="code">hospital.maintenance</field>
            <field name="prefix">HM/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="admit_patient_sequence" model="ir.sequence">
            <field name="name">Admit Patient Sequence Number</field>
            <field name="code">admit.patient</field>
            <field name="prefix">APS/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>